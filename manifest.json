{
  "manifest_version": 3,
  "name": "URL Checker",
  "version": "1.0",
  "permissions": [ "activeTab", "webNavigation", "storage", "tabs" ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
